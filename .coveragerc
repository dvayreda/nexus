[run]
source = .
omit =
    */tests/*
    */venv/*
    */.venv/*
    */env/*
    */__pycache__/*
    */site-packages/*
    setup.py
    */node_modules/*
    .git/*

branch = True

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    pass

[html]
directory = htmlcov

[xml]
output = coverage.xml
