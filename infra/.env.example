# Nexus Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# PostgreSQL
POSTGRES_USER=faceless
POSTGRES_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD
POSTGRES_DB=n8n

# n8n Authentication
N8N_USER=admin
N8N_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# code-server
CODE_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# Timezone
TIMEZONE=Europe/Madrid

# Optional: Netdata Cloud (leave empty if not using)
NETDATA_CLAIM_TOKEN=
NETDATA_CLAIM_URL=https://app.netdata.cloud

# API Keys (add as you configure them)
# PEXELS_API_KEY=
# ANTHROPIC_API_KEY=
# GROQ_API_KEY=
# INSTAGRAM_ACCESS_TOKEN=
# TELEGRAM_BOT_TOKEN=
